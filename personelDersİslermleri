      ((Control) this).Name = nameof (personel_ders_alma_verme_islemleri);
      ((Control) this).Text = "PERSONEL DERS ALMA VE VERME İŞLEMLERİ";
      ((Form) this).Load += new EventHandler(this.personel_ders_alma_verme_islemleri_Load);
      ((ISupportInitialize) this.personelBindingSource).EndInit();
      this.prometheus_projesiDataSet.EndInit();
      ((ISupportInitialize) this.dataGridView1).EndInit();
      this.ResumeLayout(false);
      ((Control) this).PerformLayout();
    }
  }
}
