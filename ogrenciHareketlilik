    private void yazdirsonBTN_Click(object sender, EventArgs e)
    {
      this.muhasebe_islem_fisi_yazdir((float) Convert.ToDouble(this.sonOdemeYuzdeTB.Text));
      this.harcama_talimati_yazdir((float) Convert.ToDouble(this.sonOdemeYuzdeTB.Text));
    }

    protected virtual void Dispose(bool disposing)
    {
      if (disposing && this.components != null)
        this.components.Dispose();
      base.Dispose(disposing);
    }

