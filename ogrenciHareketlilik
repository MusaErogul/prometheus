        catch (Exception ex)
        {
          int num = (int) MessageBox.Show(ex.Message);
          return;
        }
        if (dataTable.Rows.Count > 0)
        {
          TextBox avansMiktarTb = this.avansMiktarTB;
          double num = Convert.ToDouble(this.avansYuzdeTB.Text) * Convert.ToDouble(dataTable.Rows[0]["Odeme"].ToString()) / 100.0;
          string str1 = num.ToString();
          avansMiktarTb.Text = str1;
          TextBox sonOdemeTb = this.sonOdemeTB;
          num = Convert.ToDouble(this.sonOdemeYuzdeTB.Text) * Convert.ToDouble(dataTable.Rows[0]["Odeme"].ToString()) / 100.0;
          string str2 = num.ToString();
          sonOdemeTb.Text = str2;
          this.odemelerAvansTableAdapter.Fill(this.prometheus_projesiDataSet.odemelerAvans, new int?(Convert.ToInt32(this.hareketlilikIdTB.Text)));
          this.odemelerSonTableAdapter.Fill(this.prometheus_projesiDataSet.odemelerSon, new int?(Convert.ToInt32(this.hareketlilikIdTB.Text)));
        }
      }
      catch
      {
        this.avansMiktarTB.Text = "0";
        this.sonOdemeTB.Text = "0";
      }
