    private void avansYuzdeTB_TextChanged(object sender, EventArgs e)
    {
      try
      {
        this.sonOdemeYuzdeTB.Text = (100.0 - Convert.ToDouble(this.avansYuzdeTB.Text)).ToString();
        if (Convert.ToDouble(this.avansYuzdeTB.Text) > 100.0)
          this.avansYuzdeTB.Text = "80";
        if (Convert.ToDouble(this.avansYuzdeTB.Text) < 0.0)
          this.avansYuzdeTB.Text = "20";
      }
      catch
      {
        this.sonOdemeYuzdeTB.Text = "20";
        this.avansYuzdeTB.Text = "100";
      }
      this.hesapla();
    }

