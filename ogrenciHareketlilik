    private void ExcelDocViewer(string fileName)
    {
      try
      {
        Process.Start(fileName);
      }
      catch
      {
      }
    }

    private void harcama_talimati_yazdir(float yuzde)
    {
      SqlConnection sqlConnection = new SqlConnection(ConfigurationManager.ConnectionStrings["promethues_projesi.Properties.Settings.prometheus_projesiConnectionString"].ConnectionString);
      SqlDataAdapter sqlDataAdapter = new SqlDataAdapter();
      DataTable dataTable = new DataTable();
      SqlCommand sqlCommand = new SqlCommand("select * from odeme_fisi_avans where avansmi=1 and hareketlilik_id=@hareketlilik_id");
      sqlCommand.Connection = sqlConnection;
      sqlDataAdapter.SelectCommand = sqlCommand;
      sqlCommand.Parameters.AddWithValue("hareketlilik_id", (object) Convert.ToInt32(this.hareketlilikIdTB.Text.Trim()));
      try
      {
        sqlConnection.Open();
        sqlDataAdapter.Fill(dataTable);
        sqlConnection.Close();
      }
      catch (Exception ex)
      {
        int num = (int) MessageBox.Show(ex.Message);
        return;
      }
      if (dataTable.Rows.Count <= 0)
        return;
      Workbook workbook1 = new Workbook();
      Worksheet worksheet = workbook1.Worksheets[0];
      workbook1.Version = ExcelVersion.Version2013;
      worksheet.Range["A1"].ColumnWidth = 3.57;
      worksheet.Range["B1"].ColumnWidth = 2.0;
      worksheet.Range["C1"].ColumnWidth = 9.14;
      worksheet.Range["D1:F1"].ColumnWidth = 7.0;
      worksheet.Range["G1"].ColumnWidth = 13.43;
      worksheet.Range["H1"].ColumnWidth = 9.14;
      worksheet.Range["I1"].ColumnWidth = 8.14;
      worksheet.Range["J1"].ColumnWidth = 9.29;
      worksheet.Range["K1"].ColumnWidth = 7.0;
      worksheet.Range["L1"].ColumnWidth = 5.3;
      worksheet.Range["M1:N1"].ColumnWidth = 7.0;
      worksheet.Range["B1:N1"].Merge();
      worksheet.Range["B1"].Text = "HARCAMA TALİMATI";
      worksheet.Range["B1"].Style.HorizontalAlignment = HorizontalAlignType.Center;
      worksheet.Range["B1"].Style.Font.IsBold = true;
      worksheet.Range["C3:H3"].Merge();
      worksheet.Range["C3"].Text = "Sayı: B.30.2.AİÇ.0.76.00.00-720-";
      worksheet.Range["L3"].Merge();
      worksheet.Range["L3"].Text = "Tarih";
      worksheet.Range["M3:N3"].Merge();
      worksheet.Range["M3"].Text = DateTime.Today.ToShortDateString();
      worksheet.Range["C4:H4"].Merge();
      worksheet.Range["C4"].Text = "Harcama Talebinde Bulunan Birim: Uluslararası İlişkiler Birimi";
      worksheet.Range["B5:N5"].Merge();
      worksheet.Range["B5"].Text = "GENEL SEKRETERLİK MAKAMINA";
      worksheet.Range["B5"].Style.HorizontalAlignment = HorizontalAlignType.Center;
      worksheet.Range["B5"].Style.Font.IsBold = true;
      worksheet.Range["B6:N6"].Merge();
      worksheet.Range["B7:N7"].Merge();
      worksheet.Range["B8:B17"].Merge();
      worksheet.Range["B8"].Text = "YAPILACAK HARCAMANIN";
      worksheet.Range["B8"].Style.Rotation = 90;
      worksheet.Range["C8:F8"].Merge();
      worksheet.Range["C8"].Text = "Gerekçesi ve hukuki dayanağı";
      worksheet.Range["C8"].Style.VerticalAlignment = VerticalAlignType.Center;
      worksheet.Range["G8:N8"].Merge();
      worksheet.Range["G8"].IsWrapText = true;
      worksheet.Range["G8:N8"].Text = this.donemCB.Text + " Erasmus Plus Öğrenci Öğrenim Hareketlilik Faaliyeti Hibe Sözleşmesi Sözleşme No: 2016-1-TR01-KA103-026936 ";
      worksheet.Range["G8:N8"].RowHeight *= 2.0;
      worksheet.Range["C9:F9"].Merge();
      worksheet.Range["C9"].Text = "Konusu/nev'i / niteliği";
      worksheet.Range["G9:N9"].Merge();
      worksheet.Range["G9"].Text = " ";
      worksheet.Range["C10:F10"].Merge();
      worksheet.Range["C10"].Text = "Miktarı";
      worksheet.Range["G10:N10"].Merge();
      worksheet.Range["G10"].Text = ((double) yuzde > 50.0 ? "İlk" : "Son") + " ödeme (%" + yuzde.ToString() + ")";
      worksheet.Range["C11:F11"].Merge();
      worksheet.Range["C11"].Text = "Gerçekleştirme süresi";
      worksheet.Range["G11"].Merge();
      worksheet.Range["G11"].Text = DateTime.Parse(dataTable.Rows[0]["gidis_tarih"].ToString()).ToShortDateString();
      worksheet.Range["H11:I11"].Merge();
      CellRange cellRange1 = worksheet.Range["H11"];
      DateTime dateTime = DateTime.Parse(dataTable.Rows[0]["donus_tarihi"].ToString());
      string shortDateString1 = dateTime.ToShortDateString();
      cellRange1.Text = shortDateString1;
      worksheet.Range["J11"].Merge();
      worksheet.Range["J11"].Text = dataTable.Rows[0]["kalinacak_gun"].ToString();
      worksheet.Range["K11"].Merge();
      worksheet.Range["K11"].Text = "GÜN";
      worksheet.Range["C12:F12"].Merge();
      worksheet.Range["C12"].Text = "Gerçekleştirme usulü";
      worksheet.Range["G12:N12"].Merge();
      worksheet.Range["G12"].Text = " ";
      worksheet.Range["C13:F13"].Merge();
      worksheet.Range["C13"].Text = "Tutarı veya belirlenmişse yaklaşık bedeli";
      worksheet.Range["G13:N13"].Merge();
