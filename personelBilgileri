    }

    private void sil_Click(object sender, EventArgs e)
    {
      if (MessageBox.Show("Verileriniz Silinecektir.Verilerinizi Silmek İstediğinizden Eminmisiniz?", "Silme", MessageBoxButtons.YesNo, MessageBoxIcon.Exclamation) != DialogResult.Yes)
        return;
      this.personelBindingSource.RemoveCurrent();
      this.personelTableAdapter.Update(this.prometheus_projesiDataSet.personel);
      this.prometheus_projesiDataSet.AcceptChanges();
      int num = (int) MessageBox.Show("Personel Silindi");
    }

    private void ekle_Click(object sender, EventArgs e)
    {
      this.personelBindingSource.EndEdit();
      this.personelTableAdapter.Update(this.prometheus_projesiDataSet.personel);
      this.prometheus_projesiDataSet.AcceptChanges();
      int num = (int) MessageBox.Show("Ekleme Yapıldı");
      ((Control) this.yeniBTN).Enabled = true;
      ((Control) this.ekleBTN).Enabled = false;
    }

    private void guncelle_Click(object sender, EventArgs e)
